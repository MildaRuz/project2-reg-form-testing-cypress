{
  "name": "project2-reg-form-testing-cypress",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5177",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cypress:ui": "npx cypress open",
    "cypress:ci": "npx cypress run --browser chrome",
    "test:ci": "concurrently \"vite --port 5177\" \"cypress run --browser chrome\"",
    "test:ui": "concurrently \"vite --port 5177\" \"cypress open --browser chrome\""
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.10.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "vite": "^5.0.8"
  }
}
